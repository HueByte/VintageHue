{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "HueHordes/HueHordes/HueHordes.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "HueHordes/api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "HueHordes/api/**.yml",
          "HueHordes/api/index.md"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/toc.yml"
        ],
        "exclude": [
          "_site/**",
          "HueHordes/api/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "VintageHue Mods Documentation",
      "_appName": "VintageHue",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "repo": "https://github.com/HueByte/VintageHue",
        "branch": "master",
        "path": "docs"
      },
      "_gitUrlPattern": "github",
      "_enableDarkMode": true
    }
  }
}